<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE experiments SYSTEM "behaviorspace.dtd">
<experiments>
  <experiment name="v3w-exp1x" repetitions="50" runMetricsEveryStep="false">
    <setup>setup</setup>
    <go>go</go>
    <timeLimit steps="5000"/>
    <metric>%infected</metric>
    <steppedValueSet variable="mobility-prob" first="0" step="0.05" last="1"/>
    <enumeratedValueSet variable="agent-healing-prob">
      <value value="0.25"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="init-contamination-ratio">
      <value value="10"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="init-obstacles-ratio">
      <value value="10"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="direct-infection-weight">
      <value value="0.25"/>
    </enumeratedValueSet>
    <steppedValueSet variable="patch-contamination-prob" first="0" step="0.05" last="1"/>
    <enumeratedValueSet variable="patch-heal-prob">
      <value value="0.15"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="population">
      <value value="250"/>
    </enumeratedValueSet>
    <enumeratedValueSet variable="init-infected-ratio">
      <value value="5"/>
    </enumeratedValueSet>
    <steppedValueSet variable="patch-infection-weight" first="0" step="0.15" last="0.75"/>
    <enumeratedValueSet variable="infection-probability">
      <value value="1"/>
    </enumeratedValueSet>
  </experiment>
</experiments>
